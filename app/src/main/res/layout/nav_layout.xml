<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="@dimen/_72sdp"
    android:layout_marginBottom="@dimen/_42sdp"
    android:layout_marginStart="@dimen/_12sdp"
    android:background="@drawable/nav_background_border"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_margin="@dimen/_20sdp">



        <TextView
            android:id="@+id/navLaunchPremium"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="@dimen/_13ssp"
            android:textColor="#2C2D39"
            android:padding="@dimen/_2sdp"
            android:fontFamily="@font/ubuntu_regular"
            android:drawableStart="@drawable/best_seller_icon"
            android:drawablePadding="@dimen/_15sdp"
            android:text="@string/LaunchPremium"
            android:gravity="center_vertical"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#dadada"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_10sdp"
            />

        <TextView
            android:id="@+id/navPremiumPosts"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="@dimen/_13ssp"
            android:textColor="#2C2D39"
            android:padding="@dimen/_2sdp"
            android:fontFamily="@font/ubuntu_regular"
            android:drawableStart="@drawable/premium_post_icon"
            android:drawablePadding="@dimen/_15sdp"
            android:text="@string/premiumPosts"
            android:gravity="center_vertical"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#dadada"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_10sdp"
            />




        <TextView
            android:id="@+id/navPremiumServices"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="@dimen/_13ssp"
            android:textColor="#2C2D39"
            android:padding="@dimen/_2sdp"
            android:fontFamily="@font/ubuntu_regular"
            android:drawableStart="@drawable/ic_premium_services"
            android:drawablePadding="@dimen/_15sdp"
            android:text="Premium services"
            android:gravity="center_vertical"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#dadada"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_10sdp"
            />


        <TextView
            android:id="@+id/navPayment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="@dimen/_13ssp"
            android:textColor="#2C2D39"
            android:padding="@dimen/_2sdp"
            android:fontFamily="@font/ubuntu_regular"
            android:drawableStart="@drawable/price_icon"
            android:drawablePadding="@dimen/_15sdp"
            android:text="@string/Payments"
            android:gravity="center_vertical"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#dadada"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_10sdp"
            />


        <TextView
            android:id="@+id/navReport"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="@dimen/_13ssp"
            android:textColor="#2C2D39"
            android:padding="@dimen/_2sdp"
            android:fontFamily="@font/ubuntu_regular"
            android:drawableStart="@drawable/report_icon"
            android:drawablePadding="@dimen/_15sdp"
            android:text="Report"
            android:gravity="center_vertical"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#dadada"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_10sdp"
            />


        <TextView
            android:id="@+id/navHelp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="@dimen/_13ssp"
            android:textColor="#2C2D39"
            android:padding="@dimen/_2sdp"
            android:fontFamily="@font/ubuntu_regular"
            android:drawableStart="@drawable/help_icon"
            android:drawablePadding="@dimen/_15sdp"
            android:text="@string/Help"
            android:gravity="center_vertical"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#dadada"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_10sdp"
            />


        <TextView
            android:id="@+id/navSignOut"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="@dimen/_13ssp"
            android:textColor="#2C2D39"
            android:padding="@dimen/_2sdp"
            android:fontFamily="@font/ubuntu_regular"
            android:drawableStart="@drawable/logout_icon"
            android:drawablePadding="@dimen/_15sdp"
            android:text="@string/SignOut"
            android:gravity="center_vertical"
            />






    </LinearLayout>

</androidx.core.widget.NestedScrollView>